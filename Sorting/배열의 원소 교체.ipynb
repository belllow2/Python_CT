    "n, k = map(int, input().split())\n",
    "\n",
    "a = list(map(int, input().split()))\n",
    "b = list(map(int, input().split()))\n",
    "\n",
    "a.sort()\n",
    "b.sort()\n",
    "\n",
    "for i in range(k):\n",
    "    if a[i] < b[-i]:\n",
    "        a[i],b[-i] = b[-i],a[i]\n",
    "    else:\n",
    "        break\n",
    "\n",
    "        \n",
    "print(sum(a))"
